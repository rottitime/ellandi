{
  "name": "sensible-starter",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "^16.15.0",
    "npm": ">5.2.0",
    "yarn": "0.0.0-use-npm"
  },
  "os": [
    "linux",
    "darwin"
  ],
  "scripts": {
    "help": "xnr ./.scripts/help.ts",
    "dev:help": "echo 'Start development environment'",
    "dev": "vite",
    "prod:help": "echo 'Start production environment'",
    "prod": "vite build && xnr ./.scripts/prod.ts",
    "validate:help": "echo 'Static source code analysis'",
    "validate": "npm run types && npm run lint",
    "lint:help": "echo 'Lint all files, output to stdout'",
    "lint": "eslint .",
    "types:help": "echo 'Typecheck all TS files, output to stdout'",
    "types": "tsc && echo 'Type-checking passed'",
    "view-bundle:help": "echo 'Creates a chart showing each dependencies effect on the bundle'",
    "view-bundle": "vite build --sourcemap && source-map-explorer ./dist/assets/index.*.js --no-border-checks && rm ./dist/assets/*.js.map",
    "prepare:help": "echo 'Install (or reinstall) git hooks'",
    "prepare": "if test -d \"../.git\"; then cd .. && husky install web/.husky && rm -rf .git/hooks && ln -s ../web/.husky ./.git/hooks; fi"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/dedent": "^0.7.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/styletron-engine-atomic": "^1.1.1",
    "@types/styletron-engine-monolithic": "^0.0.0",
    "@types/styletron-react": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "1.1.3",
    "baseui": "^11.0.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "concurrently": "^7.1.0",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "express": "^4.18.0",
    "helmet": "^5.0.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "lucide-react": "^0.44.0",
    "morgan": "^1.10.0",
    "postcss-nested": "^5.0.6",
    "prettier": "2.4.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "sass": "^1.49.11",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "styletron-engine-atomic": "^1.4.8",
    "styletron-engine-monolithic": "^0.0.6",
    "styletron-react": "^6.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.8",
    "vite-plugin-pages": "^0.22.0",
    "vite-tsconfig-paths": "^3.3.17",
    "xnr": "^0.1.1"
  }
}
